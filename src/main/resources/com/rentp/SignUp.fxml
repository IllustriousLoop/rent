<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.PasswordField?>
<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.rentp.SignUpController">
    <children>
        <Label text="Name" />
        <TextField fx:id="name" />

        <Label text="Last Name" />
        <TextField fx:id="LName" />

        <Label text="Email" />
        <TextField fx:id="email" />

        <Label text="password" />
        <PasswordField fx:id="password" />

        <Button text="Sign Up"  />
        <Button text="I have account" onAction="#switchToPrimary" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>