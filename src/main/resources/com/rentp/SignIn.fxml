<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.PasswordField?>
<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.rentp.SignInController">
   <children>
      <Label fx:id="EmailLabel" text="Email"/>
      <TextField fx:id="Email"/>
      <Label fx:id="PasswordLabel" text="Password" />
      <PasswordField fx:id="Password" />

      <Button text="Sign In" onAction="#onSignInButtonClick" styleClass="btn, btn-primary"/>
      <Button text="you have not account" onAction="#switchToSecondary" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>