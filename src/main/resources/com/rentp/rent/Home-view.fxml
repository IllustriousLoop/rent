<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.rentp.rent.HomeController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>


    <Label fx:id="EmailLabel"/>
    <TextField fx:id="Email"/>
    <Label fx:id="PasswordLabel"/>
    <TextField fx:id="Password"/>

    <Button text="Sign In" onAction="#onSignInButtonClick"/>
    <Button text="Sign Up" onAction="#onSignUpButtonClick"/>
</VBox>
